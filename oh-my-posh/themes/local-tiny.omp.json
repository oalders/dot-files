{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#4C566A",
          "foreground": "#88C0D0",
          "properties": {
            "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} "
          },
          "style": "powerline",
          "type": "os"
        },
        {
          "background": "#5f819d",
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "folder_separator_icon": " \ue0b1 ",
            "home_icon": "~",
            "style": "folder",
            "template": " \uf74a  {{ .Path }} "
          },
          "style": "powerline",
          "type": "path"
        },
        {
          "background": "#83769c",
          "foreground": "#193549",
          "properties": {
            "always_enabled": true,
            "template": "<transparent>\ue0b0</> \ufa1e{{ .FormattedMs }}\u2800"
          },
          "style": "plain",
          "type": "executiontime"
        },
        {
          "background": "#8abeb7",
          "background_templates": [
            "{{ if gt .Code 0 }}#BF616A{{ end }}"
          ],
          "foreground": "#193549",
          "properties": {
            "always_enabled": true,
            "template": "<#83769c>\ue0b0</> \u2665 "
          },
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 1
}
